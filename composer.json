{
    "name": "cinexpert/tools",
    "description": "CineXpert Tools",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/cinexpert/tools",
    "require": {
        "php": ">=7.1",
        "ext-imap": "*",
        "ext-mailparse": "*",
        "ext-dom": "*",
        "laminas/laminas-servicemanager": "^3.3",
        "aws/aws-sdk-php": "^3.52",
        "pubnub/pubnub": "^4.1",
        "laminas/laminas-dependency-plugin": "^2.2",
        "laminas/laminas-mail": "^2.12",
        "php-mime-mail-parser/php-mime-mail-parser": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.2",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Cinexpert\\Tools\\": "src/Tools"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
